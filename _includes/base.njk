  <body>
    <header>
      <nav aria-label="Main">
        <div role="banner">
          <div class="blob" role="presentation"></div>
          <a href="/" class="{% if "/" == page.url %}active{% endif %}">tim von oldenburg</a>
        </div>
        <ul>
          {%- for p in collections.pages -%}
            <li {% if p.url == page.url %}aria-current="page"{% endif %}>
              <a href="{{ p.url }}">{{ p.data.title }}</a>
            </li>
            {%- endfor -%}
        </ul>
      </nav>
    </header>
    <main>
      {{ content | safe }}
    </main>

    <footer>
      <div>
        <p>© Tim von Oldenburg • hej(at)tvooo.de</p>
        <nav aria-label="Social Links"><a href="/">tvooo.de</a> • <a href="https://github.com/tvooo/">Github</a> • <a href="https://twitter.com/timvonoldenburg">Twitter</a></nav>
      </div>
    </footer>
